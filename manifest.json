{
  "manifest_version": 2,
  "name": "Procrastinate No More",
  "description": "Are you really studying?",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click here!"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [ "jquery-2.1.4.min.js", "content.js"]
  }],
  "permissions": [
    "activeTab",
    "webNavigation",
    "https://ajax.googleapis.com/",
    "storage",
    "http://*/",
    "https://*/",
    "tabs"
  ]
}
